{
  "name": "ling",
  "displayName": "ling",
  "description": "迅捷",
  "publisher": "luo-rori",
  "version": "0.0.10",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/luoyou/ling"
  },
  "activationEvents": [
    "onCommand:ling.helloWorld",
    "onCommand:ling.endSemicolon",
    "onCommand:ling.preEndSemicolon",
    "onCommand:ling.endComma",
    "onCommand:ling.preEndComma",
    "onCommand:ling.endColon",
    "onCommand:ling.endEqual",
    "onCommand:ling.endArrow",
    "onCommand:ling.endDoubleArrow",
    "onCommand:ling.endBraces",
    "onCommand:ling.endSquareBraces",
    "onCommand:ling.endParentheses",
    "onCommand:ling.completeParentHeses",
    "onLanguage:php"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ling.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "ling.endSemicolon",
        "title": "行尾分号"
      },
      {
        "command": "ling.preEndSemicolon",
        "title": "行尾分号光标前移"
      },
      {
        "command": "ling.endComma",
        "title": "行尾逗号"
      },
      {
        "command": "ling.preEndComma",
        "title": "行尾逗号光标前移"
      },
      {
        "command": "ling.endColon",
        "title": "行尾冒号"
      },
      {
        "command": "ling.endEqual",
        "title": "行尾等号"
      },
      {
        "command": "ling.endArrow",
        "title": "行尾->"
      },
      {
        "command": "ling.endDoubleArrow",
        "title": "行尾=>"
      },
      {
        "command": "ling.endBraces",
        "title": "行尾{}"
      },
      {
        "command": "ling.endSquareBraces",
        "title": "行尾[]"
      },
      {
        "command": "ling.endParentheses",
        "title": "行尾()"
      },
      {
        "command": "ling.completeParentHeses",
        "title": "行尾补全()"
      }
    ],
    "keybindings": [
      {
        "command": "ling.endSemicolon",
        "key": "alt+;",
        "mac": "alt+;",
        "when": "editorTextFocus"
      },
      {
        "command": "ling.preEndSemicolon",
        "key": "alt+ctrl+;",
        "mac": "alt+cmd+;",
        "when": "editorTextFocus"
      },
      {
        "command": "ling.endComma",
        "key": "alt+,",
        "mac": "alt+,",
        "when": "editorTextFocus"
      },
      {
        "command": "ling.preEndComma",
        "key": "alt+ctrl+,",
        "mac": "alt+ctrl+,",
        "when": "editorTextFocus"
      },
      {
        "command": "ling.endColon",
        "key": "alt+shift+;",
        "mac": "alt+shift+;",
        "when": "editorTextFocus"
      },
      {
        "command": "ling.endEqual",
        "key": "alt+=",
        "mac": "alt+=",
        "when": "editorTextFocus"
      },
      {
        "command": "ling.endArrow",
        "key": "alt+.",
        "mac": "alt+.",
        "when": "editorTextFocus"
      },
      {
        "command": "ling.endDoubleArrow",
        "key": "alt+shift+.",
        "mac": "alt+shift+.",
        "when": "editorTextFocus"
      },
      {
        "command": "ling.endBraces",
        "key": "alt+shift+[",
        "mac": "alt+shift+[",
        "when": "editorTextFocus"
      },
      {
        "command": "ling.endSquareBraces",
        "key": "alt+[",
        "mac": "alt+[",
        "when": "editorTextFocus"
      },
      {
        "command": "ling.endParentheses",
        "key": "alt+shift+9",
        "mac": "alt+shift+9",
        "when": "editorTextFocus"
      },
      {
        "command": "ling.completeParentHeses",
        "key": "alt+shift+0",
        "mac": "alt+shift+0",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3",
    "@vscode/test-electron": "^2.2.0"
  }
}
